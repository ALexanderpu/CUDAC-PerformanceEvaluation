#include "global.h"

using namespace std;
int main(int argc, char *argv[]){
	vector<vector<float>> distances;
	distances.push_back({1,24,3.0});
	distances.push_back({2.9,3,4.4});
	distances.push_back({41,5,9.2});
	vector<size_t > hello = {0,1,2};
	vector<vector<size_t>> result = rank_matrix_gpu(distances, hello);
	std::cout << result[0][2] << std::endl;
	return 0;

}
